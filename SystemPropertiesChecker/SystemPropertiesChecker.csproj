<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFrameworks>net462;net472</TargetFrameworks>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>b.ico</ApplicationIcon>
    <Version>2019.3.4.1842</Version>
    <Authors>Sebastian Walter</Authors>
    <Company>evilbaschdi</Company>
    <Copyright>Copyright © evilbaschdi 2014 - 2019</Copyright>
    <PackageProjectUrl>https://github.com/evilbaschdi/SystemPropertiesChecker</PackageProjectUrl>
    <PackageIconUrl>https://github.com/evilbaschdi/SystemPropertiesChecker/blob/master/SystemPropertiesChecker/b.ico</PackageIconUrl>
    <RepositoryUrl>https://github.com/evilbaschdi/SystemPropertiesChecker</RepositoryUrl>    
  </PropertyGroup>
  <ItemGroup>
    <None Remove="b.png" />
    <None Remove="SystemPropertiesChecker.VisualElementsManifest.xml" />
    <None Remove="b_150.png" />
    <None Remove="b_70.png" />
    <None Remove="b.ico" />
    <Content Include="b.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="SystemPropertiesChecker.VisualElementsManifest.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="b_150.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="b_70.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
     <Resource Include="b.ico">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="ControlzEx" Version="4.0.0-alpha0231" />
    <PackageReference Include="EvilBaschdi.Core" Version="2019.3.4.751-develop" />
    <PackageReference Include="EvilBaschdi.CoreExtended" Version="2019.3.3.1900-develop" />
    <PackageReference Include="MahApps.Metro" Version="2.0.0-alpha0261" />
    <PackageReference Include="MahApps.Metro.IconPacks" Version="3.0.0-alpha0103" />
    <PackageReference Include="Unity" Version="5.9.7" />
    <PackageReference Include="System.Runtime.CompilerServices.Unsafe" Version="4.6.0-preview.19073.11" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="4.6.0-preview.19073.11" />
    <PackageReference Include="System.Security.AccessControl" Version="4.6.0-preview.19073.11" />
    <PackageReference Include="System.Security.Permissions" Version="4.6.0-preview.19073.11" />
    <PackageReference Include="System.Security.Principal.Windows" Version="4.6.0-preview.19073.11" />
    <PackageReference Include="System.DirectoryServices" Version="4.6.0-preview.19073.11" />
    <PackageReference Include="System.Management" Version="4.6.0-preview.19073.11" />
    <PackageReference Include="System.DirectoryServices.AccountManagement" Version="4.6.0-preview.19073.11" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="mscorlib" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>
</Project>
